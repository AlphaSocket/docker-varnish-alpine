#!/bin/sh
#
# Run command
#
set -e

echo "### Start container configuration\n"
$(which docker-config)
echo "### Finished container configuration\n"

echo "### Start container command"
echo -n $CONFIG_VARNISH_STARTUP_OPTIONS $CONFIG_VARNISH_STARTUP_ADDITIONAL_OPTIONS | envsubst | xargs -exec varnishd 
exit $?